Algoritmo sin_titulo
	DIMENSION Cantidades[5]
	// CARGAR EL VECTOR
	Para I<-0 Hasta 4 Con Paso 1 Hacer
		Escribir "Ingrese cantidad #",I,": "
		Leer Cantidades[I]
	FinPara
	Escribir "-------------------------"
	Para I<-3 Hasta 4 Con Paso 1 Hacer
		Escribir "#",I,": ", Cantidades[I]
	FinPara
FinAlgoritmo
